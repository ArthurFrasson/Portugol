programa {
  funcao inicio() {
    real nota
    escreva("Insira a média das suas notas!\n")
    leia (nota)
    se(nota > 6.9 e nota < 10.1)
    escreva("Você está na média, parabens!")
    senao se(nota > 4.9 e nota < 7)
    escreva("Você está de recuperação!")
    senao escreva("Você reprovou!!!")
    }
  }

